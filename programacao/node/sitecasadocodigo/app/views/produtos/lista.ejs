<html>
    <title>Site Casa do Codigo</title>
    <body>
        <table>
            <tr>
                <td>ID</td>
                <td>Nome</td>
                <td>Descrição</td>
                <td>Preço</td>
            </tr>

            <%lista.forEach(function (produto) {%>
                <tr>
                    <td><%=produto.id%></td>
                    <td><%=produto.titulo%></td>
                    <td><%=produto.descrica%></td>
                    <td><%=produto.preco%></td>
                </tr>
            <%})%>

        </table>
    </body>
</html>